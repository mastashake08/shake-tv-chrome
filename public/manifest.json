{
    "manifest_version": 3,
    "name": "Shake TV",
    "version": "1.0",
    "description": "Stream IPTV content with Video.js",
    "permissions": ["activeTab", "storage"],
    "host_permissions": [
      "https://iptv-org.github.io/*"
    ],
   
    "action": {
      "default_popup": "index.html",
      "default_icon": "vite.svg"
    }
  }
  